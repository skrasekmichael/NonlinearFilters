<Application x:Class="NonlinearFilters.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:converters="clr-namespace:NonlinearFilters.APP.Converters"
			 xmlns:templateSelectors="clr-namespace:NonlinearFilters.APP.TemplateSelectors">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
			</ResourceDictionary.MergedDictionaries>

			<templateSelectors:FilterParameterTemplateSelector x:Key="FilterParameterTemplateSelector"/>
			
			<converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
			<converters:DoubleToStringConverter x:Key="DoubleToStringConverter"/>
			<converters:IntToStringConverter x:Key="IntToStringConverter"/>
			<converters:ClassToNameConverer x:Key="ClassToNameConverer"/>
			<converters:EnumTypeToArrayConverter x:Key="EnumTypeToArrayConverter"/>
		</ResourceDictionary>
	</Application.Resources>
</Application>
